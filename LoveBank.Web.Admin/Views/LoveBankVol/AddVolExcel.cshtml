                <tr>
                    <td class="item_title">选择批量导入文件</td>
                    <td class="item_input"><input type="file" name="volInfoExcel" />(批量导入的用户默认密码:111111)</td>
                </tr>

                <tr>
                    <td class="item_title"></td>
                    <td class="item_input">

                        <a href="javascript:void(0)" class="easyui-linkbutton" onclick="submitForm()">导入</a>
          
                    </td>
                </tr>
             
            </tbody>
        </table>
        <script>
            function submitForm() {
           
                $('#PostAddVolExcel').submit();
            }
         

         
        </script>
    </form>
    @Html.IncludeScript("~/Content/EasyUI/jquery.easyui.min.js")
</div>