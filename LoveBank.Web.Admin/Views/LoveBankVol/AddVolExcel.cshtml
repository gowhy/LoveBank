@using LoveBank.Common;
@using LoveBank.MVC
<div class="main">

    <form name="PostAddVolExcel" id="PostAddVolExcel" action="@Url.Action("PostAddVolExcel")" method="post" enctype="multipart/form-data">
        <table class="form" cellpadding="0" cellspacing="0">
            <tbody>
                <tr>
                    <td class="item_title">批量导入志愿者模板</td>
                    <td class="item_input"><a   title="下载爱心银行志愿者批量导入模板,用于爱心银行志愿者批量导入,请不要随便修改模板文件" href="@Url.Content("~/Content/爱心银行志愿者批量导入模板.xls")">点击下载志愿者批量导入模板</a>(用于爱心银行志愿者批量导入)</td>
